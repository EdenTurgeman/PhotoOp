{
  "name": "photo-op",
  "version": "0.9.4",
  "private": true,
  "productName": "PhotoOp,",
  "main": "src/electron/main.js",
  "dependencies": {
    "electron-store": "^3.2.0",
    "exiftool-vendored": "^18.6.0",
    "fs-extra": "^8.0.1",
    "recursive-readdir": "^2.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-electron": "electron-builder build",
    "build-app": "react-scripts build && electron-builder build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron src/electron/main.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "electron-builder": "^20.41.0",
    "prop-types": "latest",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^11.0.3",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-scripts": "^3.0.1",
    "redux": "^4.0.1",
    "styled-components": "^4.2.1"
  },
  "build": {
    "asar": true,
    "productName": "PhotoOp",
    "appId": "com.et.photoOp",
    "electronVersion": "5.0.1",
    "files": [
      "build/",
      "./src/electron/",
      "./src/assets/",
      "package.json"
    ],
    "extends": null,
    "compression": "maximum",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "output": "release"
    }
  },
  "author": {
    "name": "Eden Turgeman",
    "email": "edmx0.et@gmail.com",
    "url": "https://github.com/EdenTurgeman/"
  },
  "license": "MIT",
  "keywords": [
    "electron"
  ],
  "homepage": "./"
}
